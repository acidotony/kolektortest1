pipeline {
    environment {
    registry = "acidotony/kolektortest1"
    registryCredential = 'dockerhub'
    dockerImage = ''

    }
    agent {
        docker {
            image "maven:3.6.0-jdk-8"
            label "docker"
            args "-v /tmp/maven:/.mvn -e MAVEN_CONFIG=/.mvn"
        }
    }

    stages {
       stage('Cloning Git') {
        steps {
        git 'https://github.com/acidotony/kolektortest1.git'
      }
    }
    stage("Build") {
            steps {
                sh "mvn -version"
                sh "mvn clean install"
            }
        }
    //stage('Building image') {
      //steps{
        //script {
          //dockerImage = docker.build registry + ":$BUILD_NUMBER"
        //}
      //}
    //}
    //stage('Deploy Image') {
      //steps{
        //script {
          //docker.withRegistry( '', registryCredential ) {
            //dockerImage.push()
          //}
        //}
      //}
    }
  }
} 
